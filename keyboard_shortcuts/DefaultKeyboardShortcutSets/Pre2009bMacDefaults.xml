            <Stroke code="VK_END" ctrl="on" shift="on"/>
        </Action>
        <Action id="clear-text-at-prompt">
            <Stroke code="VK_U" ctrl="on"/>
        </Action>
        <Action id="selection-previous-word">
            <Stroke code="VK_LEFT" ctrl="on" shift="on"/>
            <Stroke code="VK_KP_LEFT" ctrl="on" shift="on"/>
        </Action>
        <Action follows="None" id="selection-begin">
            <Stroke code="VK_HOME" ctrl="on" shift="on"/>
        </Action>
        <Action id="caret-up" follows="None"/>
        <Action id="caret-down" follows="None"/>
    </Context>
</CustomKeySet>